{
    "rules": {
        "indent": [
            2,
            4, {
                "SwitchCase": 1
            }
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "strict": 2,
        "camelcase": 1,
        "curly": 1,
        "eqeqeq": 2,
        "no-alert": 2,
        "guard-for-in": 1,
        "no-debugger": 2
    },
    "ecmaFeatures": {
        "arrowFunctions": true,
        "classes": true,
        "destructuring": true,
        "forOf": true,
        "generators": true,
        "superInFunctions": true
    },
    "env": {
        "amd": true,
        "jasmine": true,
        "jquery": true,
        "es6": true,
        "browser": true,
        "node": true,
        "worker": true
    },
    "extends": "eslint:recommended"
}
